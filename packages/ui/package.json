{
  "name": "@eoms/ui",
  "version": "0.0.0",
  "license": "ISC",
  "sideEffects": false,
  "main": "./index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.cjs"
  },
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./es/index.mjs",
      "types": "./es/index.d.ts"
    },
    "./dist": "./dist/",
    "./theme": "./theme/",
    "./es": "./es/index.mjs",
    "./es/*": "./es/*.mjs",
    "./es/*.mjs": "./es/*.mjs",
    "./lib": "./lib/index.cjs",
    "./lib/*": "./lib/*.js",
    "./lib/*.cjs": "./lib/*.cjs",
    "./*": "./*"
  },
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "style": "dist/index.css",
  "scripts": {
    "build": "run-p build:**",
    "build:cjs": "vite build --config ./ViteConfigCJS.ts",
    "build:es": "vite build --config ./ViteConfigES.ts",
    "build:umd": "vite build --config ./ViteConfigUMD.ts",
    "lint": "TIMING=1 eslint ./**/*.ts* --fix",
    "clean": "run-p clean:**",
    "clean:cache": "rimraf .turbo && rimraf dist",
    "clean:deps": "rimraf node_modules"
  },
  "dependencies": {
    "@eoms/constant": "workspace:*",
    "@eoms/vite-config": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eoms/eslint-config": "workspace:*",
    "@eoms/ts-config": "workspace:*",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "vite": "^3.1.3"
  },
  "engines": {
    "node": ">=16"
  }
}
